# Production Environment Variables for Vercel
# Copy these to Vercel Environment Variables

# Database (Use Vercel Postgres or external PostgreSQL)
DATABASE_URL="postgresql://username:password@host:5432/propfirm_copier"

# NextAuth (IMPORTANT: Change these for production)
NEXTAUTH_URL="https://your-app-name.vercel.app"
NEXTAUTH_SECRET="your-production-secret-key-32-chars-min"

# Google OAuth (Create production credentials)
GOOGLE_CLIENT_ID="your-production-google-client-id"
GOOGLE_CLIENT_SECRET="your-production-google-client-secret"

# Trading APIs (Optional - can be added later)
META_API_TOKEN="your-meta-api-token"
CTRADER_CLIENT_ID="your-ctrader-client-id"
CTRADER_CLIENT_SECRET="your-ctrader-client-secret"
CTRADER_ACCESS_TOKEN="your-ctrader-access-token"

# Payment Integration (Optional - can be added later)
LEMON_SQUEEZY_API_KEY="your-lemon-squeezy-api-key"
LEMON_SQUEEZY_STORE_ID="your-store-id"

# Admin Configuration
ADMIN_EMAILS="your-email@example.com"
