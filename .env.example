CTRADER_ACCESS_TOKEN="your-ctrader-access-token"

# Match Trader API
MATCH_TRADER_API_KEY="your-match-trader-api-key"
MATCH_TRADER_SERVER="https://api.matchtrader.com"

# TradeLocker API
TRADELOCKER_EMAIL="your-tradelocker-email"
TRADELOCKER_PASSWORD="your-tradelocker-password"
TRADELOCKER_SERVER="your-tradelocker-server"

# Lemon Squeezy (for payments)
LEMON_SQUEEZY_API_KEY="your-lemon-squeezy-api-key"
LEMON_SQUEEZY_STORE_ID="your-store-id"
LEMON_SQUEEZY_WEBHOOK_SECRET="your-webhook-secret"

# Forex Data API (example: Alpha Vantage, Fixer.io, etc.)
FOREX_API_KEY="your-forex-data-api-key"

# Admin Configuration
ADMIN_EMAILS="admin@propfirmcopier.com,support@propfirmcopier.com"

